    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Company>Awaoa</Company>
    <Authors>Awaoa</Authors>
    <AssemblyName>Awaoa.Core</AssemblyName>
    <RootNamespace>Awaoa.Core</RootNamespace>
    <PackageProjectUrl>https://github.com/awaoa</PackageProjectUrl>
